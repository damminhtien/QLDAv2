<h2>Đồ án mới nhất</h2>
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>Tên đề tài</th>
            <th>Sinh viên</th>
            <th>Chi tiết</th>
        </tr>
    </thead>
    <tbody>
        <% doanmoinhat.forEach((data)=>{ %>
            <tr>
                <td><%=data.ten_de_tai%></td>
                <td><%=data.ten_sv%></td>
                <td><a href="/doan/<%=data.ma_da%>" title="Xem chi tiết"><span class="btn fa fa-eye"></span></a></td>
            </tr>
        <% }) %>
    </tbody>
</table>